{
  "extends": ["react-app", "react-app/jest", "airbnb-typescript", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/prop-types": "off",
    "react-hooks/exhaustive-deps": "off",
    "react/require-default-props": "off"
  }
}
